{
  "name": "talent-retainer-service",
  "license": "AGPL-3.0-only",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d dist --source-maps",
    "watch": "babel src -w -d dist --source-maps",
    "depcheck": "depcheck",
    "test-ci": "ENV_NAME=test jest --ci --reporters=jest-junit --outputFile=./test-results/jest",
    "start": "node dist/ports/http/bin/www",
    "test": "ENV_NAME=test jest",
    "lint": "eslint -c .eslintrc src/**/*.js",
    "local-lambda": "serverless offline -s dev",
    "prebuild-lambda": "yarn build",
    "build-lambda": "serverless package",
    "stryker-mutate": "stryker run",
    "doc": "jsdoc ./src/**/*.js  -d ./docs -c ./jsdoc.conf.json"
  },
  "jest": {
    "verbose": true
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "./test-results/jest",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "engines": {
    "node": "^12.20.0",
    "yarn": "^1.22.5",
    "npm": "^6.14.8"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.582.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cuid": "^2.1.8",
    "debug": "^4.1.1",
    "escriba": "^2.5.0",
    "express": "^4.17.1",
    "log4js": "^5.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.21",
    "ramda": "^0.26.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.8.7",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/preset-env": "^7.2.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@stryker-mutator/core": "^3.2.4",
    "@stryker-mutator/javascript-mutator": "^3.2.4",
    "@stryker-mutator/jest-runner": "^3.2.4",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.4",
    "@types/ramda": "0.26.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "commitlint": "^11.0.0",
    "depcheck": "^0.9.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^1.3.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^8.1.0",
    "prettier-eslint": "8.8.2",
    "rimraf": "^3.0.0",
    "serverless": "^2.18.0",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-jetpack": "^0.10.6",
    "serverless-localstack": "^0.4.24",
    "serverless-offline": "^12.0.3",
    "standard": "^14.3.1"
  },
  "resolutions": {
    "handlebars": "^4.3.0",
    "eslint-utils": "^1.4.1",
    "lodash": "^4.17.15",
    "minimist": "^1.2.2",
    "dd-trace": "^0.14.0",
    "mkdirp": "^1.0.3",
    "typescript": ">=2.8.0",
    "debug": "^4.1.1",
    "yargs-parser": "^18.1.2",
    "bl": ">=4.0.3",
    "node-fetch": ">=2.6.1",
    "dot-prop": ">=4.2.1",
    "axios": ">=0.21.1",
    "node-notifier": ">=8.0.1"
  }
}
